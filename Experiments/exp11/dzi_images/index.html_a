<html>
<head>
    <title>OpenSeadragon 3x3 Grid Example</title>
    <script src="https://openseadragon.github.io/openseadragon-binaries/openseadragon.min.js"></script>
</head>
<body>
    <div id="openseadragon-viewer"></div>
    <script>
    var viewer = OpenSeadragon({
        id: "openseadragon-viewer",
        tileSources: {
            type: 'image',
            url: 'https://openseadragon.github.io/example-images/grand-canyon-landscape-overlooking.jpg'
        }
    });
    </script>
</body>
</html>
